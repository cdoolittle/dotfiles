[user]
    name = Phillip Verheyden
    email = pverheyden@gmail.com
[core]
    autocrlf = input
    safecrlf = true
    editor = vim 
[alias]
    co = checkout
    ci = commit
    st = status
    br = branch
    hist = log --pretty=format:\"%h %ad [%an] | %s%d\" --graph --decorate --date=short
    type = cat-file -t
    dump = cat-file -p
[push]
    default=tracking
